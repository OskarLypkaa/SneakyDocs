{
  "name": "collapse-docs",
  "displayName": "Collapse Docs",
  "description": "Quickly collapse or uncollapse all docstrings and JSDoc comments via right-click menu.",
  "publisher": "Oskarlypka",
  "version": "1.2.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "collapseDocs.run",
        "title": "Collapse All Docstrings / JSDoc"
      },
      {
        "command": "collapseDocs.unfold",
        "title": "Uncollapse All Docstrings / JSDoc"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "collapseDocs.run",
          "when": "editorLangId == python || editorLangId == javascript || editorLangId == typescript",
          "group": "navigation"
        },
        {
          "command": "collapseDocs.unfold",
          "when": "editorLangId == python || editorLangId == javascript || editorLangId == typescript",
          "group": "navigation"
        }
      ]
    }
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "icon": "images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "vscode": "^1.1.37"
  }
}